{
  "name": "pro-mern-stack",
  "version": "1.0.0",
  "description": "Pro MERN Stack book app by Vasan Subramanian",
  "main": "start_hook.js",
  "scripts": {
    "start": "nodemon -w dist dist/server.js",
    "compile-server": "babel server --presets @bebel/preset-env --out-dir dist --source-maps",
    "watch-server": "babel server --presets @bebel/preset-env --out-dir dist --source-maps --watch",
    "start-hook": "nodemon -w server server/start_hook.js",
    "compile": "webpack --mode=development",
    "watch": "webpack-dev-server --hot --inline",
    "dev-all": "npm run watch & npm run watch-server &npm start",
    "dev-all-hook": "npm run watch & npm run start-hook",
    "lint": "eslint --ext js,jsx src server webpack.config.js"
  },
  "keywords": [
    "app",
    "book",
    "learning"
  ],
  "author": "Francisco Herrera",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^3.3.7",
    "co": "^4.6.0",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "mongodb": "^3.1.4",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-router-dom": "^4.3.1",
    "source-map-support": "^0.5.9",
    "webpack": "^4.19.1",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.24.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.2",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.3",
    "react-bootstrap": "^0.32.4",
    "react-router": "^4.3.1",
    "react-router-bootstrap": "^0.24.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "whatwg-fetch": "^3.0.0"
  }
}
